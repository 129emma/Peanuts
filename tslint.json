{
    "extends": [
        "tslint-react",
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {
    },
    "rules": {
        "member-access": false,
        "ordered-imports": false,
        "quotemark": false,
        "no-console": false,
        "semicolon": false,
        "prefer-const": {
            "severity": "warning"
        },
        "no-empty": {
            "severity": "warning"
        },
        "interface-name": {
            "severity": "warning",
            "options": "never-prefix"
        },
        "jsdoc-format": {
            "severity": "warning"
        },
        "jsx-no-multiline-js": false,
        "eofline": false,
        "object-literal-sort-keys": false

    },
    "rulesDirectory": [
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    }
}